# API Keys (REQUIRED)
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Qdrant Vector Database (Local Docker)
QDRANT_URL=http://qdrant:6333

# Redis Cache (Shared persistent cache for all users)
# Using existing hirehub-redis container via host.docker.internal
REDIS_URL=redis://host.docker.internal:6379/0

# Parakeet Self-Hosted Speech-to-Text
# Set to 'true' to use Parakeet instead of OpenAI Whisper
USE_PARAKEET=false
PARAKEET_URL=http://parakeet:8002
