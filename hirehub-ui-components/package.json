{
  "name": "hirehub-ui-components",
  "version": "1.0.0",
  "description": "HireHub UI Component Library - A comprehensive collection of 56 TypeScript-strict Vue 3 components",
  "type": "module",
  "main": "./components/base/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "components",
    "types",
    "assets"
  ],
  "exports": {
    ".": {
      "import": "./components/base/index.js",
      "types": "./types/index.d.ts"
    },
    "./components/*": "./components/base/*.vue",
    "./assets/*": "./assets/*",
    "./types/*": "./types/*"
  },
  "scripts": {
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@tiptap/vue-3": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "@tiptap/extension-underline": "^2.12.0",
    "@tiptap/extension-placeholder": "^2.12.0",
    "@tiptap/pm/state": "^2.0.0",
    "@tiptap/pm/view": "^2.0.0",
    "@tiptap/core": "^2.12.0",
    "v-calendar": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "sass": "^1.83.4",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.2"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "ui",
    "typescript",
    "composition-api",
    "hirehub",
    "design-system"
  ],
  "author": "HireHub Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/hirehub-ui-components"
  }
}
